        <attr name="blt_checkedColor" format="color"/>
        <attr name="blt_radius" format="dimension"/>
        <attr name="blt_shape">
            <enum name="rectangle" value="0"/>
            <enum name="oval" value="1"/>
        </attr>
        <attr name="blt_strokeWidth" format="dimension"/>
        <attr name="blt_isRadioButton" format="boolean"/>
        <attr name="blt_isFillBackground" format="boolean"/>
        <attr name="blt_isUnCheckedShowBorder" format="boolean"/>
