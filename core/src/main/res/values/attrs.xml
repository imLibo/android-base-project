            <enum name="rectangle" value="0"/>
            <enum name="oval" value="1"/>
        </attr>
        <attr name="blb_strokeWidth" format="dimension"/>
        <attr name="blb_pressedColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="StepProgressView">
        <attr name="spv_textSize" format="dimension"/>
        <attr name="spv_minStepWidth" format="dimension"/>
