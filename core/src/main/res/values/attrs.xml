        <attr name="lvl_valueTextEnabledEdit" format="boolean"/>
        <attr name="lvl_rightDrawable" format="reference"/>
        <attr name="lvl_rightDrawableText" format="string"/>
        <attr name="lvl_rightDrawableTextColor" format="color"/>
        <attr name="lvl_rightBackgroundColor" format="color"/>
        <attr name="lvl_rightDrawableWidth" format="dimension"/>
