<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AutoBackgroundButton">
        <attr name="abb_pressColor" format="color"/>
        <attr name="abb_normalColor" format="color"/>
        <attr name="abb_disableColor" format="color"/>
        <attr name="abb_radius" format="dimension"/>
        <attr name="abb_shape">
            <enum name="rectangle" value="0"/>
            <enum name="oval" value="1"/>
        </attr>
        <attr name="abb_textPressColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="AutoGradientButton">
        <attr name="agb_startColor" format="color"/>
        <attr name="agb_endColor" format="color"/>
        <attr name="agb_orientation">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr name="agb_gradientRadius" format="dimension"/>
        <attr name="agb_disableColor" format="color"/>
        <attr name="agb_radius" format="dimension"/>
        <attr name="agb_textPressColor" format="color"/>
        <attr name="agb_shape">
            <enum name="rectangle" value="0"/>
            <enum name="oval" value="1"/>
        </attr>
    </declare-styleable>

